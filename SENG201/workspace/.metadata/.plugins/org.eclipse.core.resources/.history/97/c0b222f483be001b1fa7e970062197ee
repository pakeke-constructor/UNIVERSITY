	private static ArrayList<ArrayList<int[]>> pathDataMatrixX = new ArrayList<ArrayList<int[]>>();
	private static ArrayList<ArrayList<int[]>> pathDataMatrixY = new ArrayList<ArrayList<int[]>>();
	
	public ArrayList<int[]> pathX;
	public ArrayList<int[]> pathY;
	
	/*
	 * Initializes with empty image
	 */
	public Path() {
		super(GameData.getEmpty(), 0, 0);
	}
	
	/*
	 * sets it's own image to the appropriate one to match the traversal between
	 * island ID 1 and island ID 2.
	 * 
	 * if any of the island ID's are -1, sets to empty path.
	 * Also, if the island ID's are equal, an empty path is drawn.
	 * 
	 * @param fromIsland the island to start from
	 * @param toIsland the island to go to
	 * 
	 */
	public void setPath(int fromIsland, int toIsland) {
		if ((fromIsland == -1) || (toIsland == -1) || (toIsland == fromIsland)) {
			setImage(GameData.getEmpty()); // set to empty image
			return;
		}
		setImage(GameData.getPathImage(fromIsland, toIsland));
	}
	
	/*
     * Draws the appropriate path image.
     * This method was initially overridden and drew the path manually,
     * however we figured it looked better with already-drawn paths.
     * 
     * @param g graphics object from GameLogic
     * @param window GameLogic object from GameLogic
     */
    @Override
    public void draw(Graphics g, GameLogic window) {
        /*
        // This is the old code that implemented continuous path rendering. 
        // We scrapped it because it was giving us trouble with island positions, and looked worse.
        
        if ((pathX == null) || (pathY == null)) {
            return;
        }
        Graphics2D g2 = (Graphics2D) g;
        int curPosX = 0;
        int curPosY = 0;
        for (int i=1; i < pathX.length; i++) {
            Line2D lin = new Line2D.Float(pathX[i-1], pathY[i-1], pathX[i]*2, pathY[i]*2);
            g2.draw(lin);
        }*/
        g.drawImage(getImage(), getX(), getY(), window);
    }
	
	/*
	 * Adds all paths to appropriate data structures
	 * 
	 */
	public static void constructPathMatrix() {
		ArrayList<int[]> island0PathX = new ArrayList<int[]>();
		ArrayList<int[]> island0PathY = new ArrayList<int[]>();
		island0PathX.add(new int[0]);
		island0PathY.add(new int[0]);
		island0PathX.add(pathDataX01);
		island0PathY.add(pathDataY01);
		island0PathX.add(pathDataX02);
		island0PathY.add(pathDataY02);
		island0PathX.add(pathDataX03);
		island0PathY.add(pathDataY03);
		island0PathX.add(pathDataX04);
		island0PathY.add(pathDataY04);
		pathDataMatrixX.add(island0PathX);
		pathDataMatrixY.add(island0PathY);
		ArrayList<int[]> island1PathX = new ArrayList<int[]>();
		ArrayList<int[]> island1PathY = new ArrayList<int[]>();
		island1PathX.add(reverse(pathDataX01));
		island1PathY.add(reverse(pathDataY01));
		island1PathX.add(new int[0]);
		island1PathY.add(new int[0]);
		island1PathX.add(pathDataX12);
		island1PathY.add(pathDataY12);
		island1PathX.add(pathDataX13);
		island1PathY.add(pathDataY13);
		island1PathX.add(pathDataX14);
		island1PathY.add(pathDataY14);
		pathDataMatrixX.add(island1PathX);
		pathDataMatrixY.add(island1PathY);
		ArrayList<int[]> island2PathX = new ArrayList<int[]>();
		ArrayList<int[]> island2PathY = new ArrayList<int[]>();
		island2PathX.add(reverse(pathDataX02));
		island2PathY.add(reverse(pathDataY02));
		island2PathX.add(reverse(pathDataX12));
		island2PathY.add(reverse(pathDataY12));
		island2PathX.add(new int[0]);
		island2PathY.add(new int[0]);
		island2PathX.add(pathDataX23);
		island2PathY.add(pathDataY23);
		island2PathX.add(pathDataX24);
		island2PathY.add(pathDataY24);
		pathDataMatrixX.add(island2PathX);
		pathDataMatrixY.add(island2PathY);
		ArrayList<int[]> island3PathX = new ArrayList<int[]>();
		ArrayList<int[]> island3PathY = new ArrayList<int[]>();
		island3PathX.add(reverse(pathDataX03));
		island3PathY.add(reverse(pathDataY03));
		island3PathX.add(reverse(pathDataX13));
		island3PathY.add(reverse(pathDataY13));
		island3PathX.add(reverse(pathDataX23));
		island3PathY.add(reverse(pathDataY23));
		island3PathX.add(new int[0]);
		island3PathY.add(new int[0]);
		island3PathX.add(pathDataX34);
		island3PathY.add(pathDataY34);
		pathDataMatrixX.add(island3PathX);
		pathDataMatrixY.add(island3PathY);
		ArrayList<int[]> island4PathX = new ArrayList<int[]>();
		ArrayList<int[]> island4PathY = new ArrayList<int[]>();
		island4PathX.add(reverse(pathDataX04));
		island4PathY.add(reverse(pathDataY04));
		island4PathX.add(reverse(pathDataX14));
		island4PathY.add(reverse(pathDataY14));
		island4PathX.add(reverse(pathDataX24));
		island4PathY.add(reverse(pathDataY24));
		island4PathX.add(reverse(pathDataX34));
		island4PathY.add(reverse(pathDataY34));
		island4PathX.add(new int[0]);
		island4PathY.add(new int[0]);
		pathDataMatrixX.add(island4PathX);
		pathDataMatrixY.add(island4PathY);
	}
	
	/*
	 * reverses a list of integers
	 * 
	 * @param data The integer list to reverse
	 * @return the reversed list
	 * 
	 */
	public static int[] reverse(int[] data) {
		int[] reversedData = new int[data.length];
		int j = data.length;
		for(int i =0; i<data.length;i++) {
			reversedData[j-1] = data[i] * -1;
			j = j- 1;
		}
		return reversedData;
	}
	
	/*
	 * Returns an array of integers representing the direction
	 * the ship should take between two islands on the X axis.
	 * 
	 * @param currentIsland the first island ID (integer)
	 * @param targetIsland the second island ID (integer)
	 * 
	 * @return an array of integers denoting the path that the ship should take in X
	 */
	public static int[] getXPath(int currentIsland, int targetIsland) {
		return pathDataMatrixX.get(currentIsland).get(targetIsland);
	}
	
	/*
	 * Returns an array of integers representing the direction
	 * the ship should take between two islands on the Y axis.
	 * 
	 * @param currentIsland the first island ID (integer)
	 * @param targetIsland the second island ID (integer)
	 * 
	 * @return an array of integers denoting the path that the ship should take in Y
	 */
	public static int[] getYPath(int currentIsland, int targetIsland) {
		return pathDataMatrixY.get(currentIsland).get(targetIsland);
	}
	
}
